{
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgresql;Port=5432;Database=application2"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8091"
      }
    }
  },
  "Secrets": {
    "Manager" : "conjur",
    "Conjur": {
      "Config": {
        "ApplianceUrl": "https://conjur:443",
        "Account": "deathshadow"
      },
      "Provider": {
        "ConnectionStrings": {
          "DefaultConnection": {
            "UserNamePath": "app/application2/dev/credentials/db-rw/login",
            "PasswordPath": "app/application2/dev/credentials/db-rw/password"
          }
        }
      }

    }
  }
}